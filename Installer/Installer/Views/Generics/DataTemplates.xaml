<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:Installer.ViewModels.Implementation"
                    xmlns:installVM="clr-namespace:Installer.ViewModels.Implementation.Install"
                    xmlns:uninstallVM="clr-namespace:Installer.ViewModels.Implementation.Uninstall"
                    xmlns:view="clr-namespace:Installer.Views"
                    xmlns:installView="clr-namespace:Installer.Views.Install"
                    xmlns:uninstallView="clr-namespace:Installer.Views.Uninstall">
    <DataTemplate DataType="{x:Type installVM:InstallLandingPageViewModel}">
        <installView:InstallLandingPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type uninstallVM:UninstallLandingPageViewModel}">
        <uninstallView:UninstallLandingPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:SummaryPageViewModel}">
        <view:SummaryPage />
    </DataTemplate>
</ResourceDictionary>